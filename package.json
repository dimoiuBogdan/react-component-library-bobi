{
  "name": "react-component-library-bobi",
  "version": "0.1.4",
  "description": "A simple React component library with TypeScript and Vite",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "start:example": "vite --config example/vite.config.ts",
    "build": "vite build && tsc -p tsconfig.build.json",
    "lint": "eslint --ignore-pattern 'dist/' --ext ts,tsx src/ *.ts --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "devDependencies": {
    "@types/node": "^22.13.9",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0",
    "vite": "^6.2.0"
  },
  "keywords": [
    "react",
    "component",
    "library",
    "typescript",
    "vite"
  ],
  "author": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bobi-dev-hub/react-component-library-bobi.git"
  },
  "license": "MIT",
  "directories": {
    "example": "example"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=16.0.0"
  }
}
